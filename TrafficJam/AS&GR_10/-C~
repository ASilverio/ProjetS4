( TAILLE_BARRE_PARTIE = 40;) p n
(const) K
( ) p
(int) k
( ECART_HORIZONTAL_ROUTES=\(LARGEUR_FENETRE-TAILLE_ROUTE*NB_ROUTES_MAX/2\)/\(NB_ROUTES_MAX/2+1\);) p n
(const) K
( ) p
(int) k
( ECART_VERTICAL_ROUTES=\(HAUTEUR_FENETRE-TAILLE_ROUTE*NB_ROUTES_MAX/2\)/\(NB_ROUTES_MAX/2+1\);) p n
(const) K
( ) p
(int) k
( TAILLE_LONGUEUR_VOITURE_ET_POLICE=66;) p n
(const) K
( ) p
(int) k
( TAILLE_LARGEUR_VOITURE_ET_POLICE=30;) p n
(const) K
( ) p
(int) k
( TAILLE_LONGUEUR_CAMION=80;) p n
(const) K
