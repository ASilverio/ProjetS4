(#ifndef) K
( _CONSTANTES_H_) p n
(#define) K
( _CONSTANTES_H_) p n
() N
(const) K
( ) p
(int) k
( LARGEUR_FENETRE=800;) p n
(const) K
( ) p
(int) k
( HAUTEUR_FENETRE=600;) p n
(const) K
( ) p
(int) k
( TAILLE_LARGEUR_BOUTON = 250;) p n
(const) K
( ) p
(int) k
( TAILLE_HAUTEUR_BOUTON = 75;) p n
(const) K
( ) p
(int) k
( NB_ROUTES_MAX=6;) p n
(const) K
( ) p
(int) k
( TAILLE_ROUTE=40;) p n
(const) K
( ) p
(int) k
( TAILLE_LARGEUR_FEU = 25;) p n
(const) K
( ) p
(int) k
( TAILLE_HAUTEUR_FEU = 49;) p n
(const) K
